{
    "private": true,
    "engines": {
        "node": "14.x",
        "npm": "6.x"
    },
    "scripts": {
        "dev": "npm run js-dev",
        "js-dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "css-dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack.css.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run js-dev -- --watch",
        "css-watch": "npm run css-dev -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
        "js-prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "css-prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --env.mixfile=webpack.css.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run js-prod && npm run css-prod",
        "heroku-postbuild": "npm run prod"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.32",
        "@fortawesome/free-brands-svg-icons": "^5.15.1",
        "@fortawesome/free-solid-svg-icons": "^5.15.1",
        "@fortawesome/vue-fontawesome": "^2.0.0",
        "@inertiajs/inertia": "^0.8.2",
        "@inertiajs/inertia-vue": "^0.1.2",
        "@tailwindcss/ui": "^0.1.3",
        "axios": "^0.21",
        "compression-webpack-plugin": "^6.0.2",
        "cross-env": "^7.0",
        "dotenv": "^8.2.0",
        "laravel-jetstream": "^0.0.3",
        "laravel-mix": "^5.0.9",
        "laravel-mix-alias": "^1.0.2",
        "localStorage": "^1.0.4",
        "lodash": "^4.17.19",
        "portal-vue": "^2.1.7",
        "postcss-import": "^12.0.1",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "tailwindcss": "^1.3.0",
        "tailwindcss-aspect-ratio": "^3.0.0",
        "to-json-schema": "^0.2.5",
        "v-click-outside": "^3.1.2",
        "v-jsoneditor": "^1.4.1",
        "vue": "^2.5.17",
        "vue-click-outside": "^1.1.0",
        "vue-disqus": "^4.0.1",
        "vue-form-generator": "^2.3.4",
        "vue-gtag": "^1.10.0",
        "vue-lazyload": "^1.3.3",
        "vue-publisher": "^1.4.4",
        "vue-template-compiler": "^2.6.10",
        "vue-vanilla-datetime-picker": "^1.1.15",
        "vue2-scrollspy": "^2.3.1",
        "webpack-chunk-rename-plugin": "^1.0.3",
        "ziggy-js": "^0.9.4"
    },
    "dependencies": {
        "puppeteer": "^5.5.0"
    }
}
